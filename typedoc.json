{
    "router": "module",
    "plugin": ["typedoc-plugin-markdown"],
    "out": "./docs",
    "useCodeBlocks": true,
    "parametersFormat": "table",
    "sort": [
        "kind",
        "source-order",
        "instance-first",
        "alphabetical-ignoring-documents"
    ],
    "tableColumnSettings": {
        "hideDefaults": true
    }
}