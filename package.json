{
    "name": "jsdom-fragments",
    "version": "1.3.0",
    "author": "dima117a@gmail.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/dima117/jsdom-fragments"
    },
    "license": "MIT",
    "typings": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "test": "jest",
        "format": "prettier --write ./**/*.ts",
        "build": "vite build",
        "prepare": "npm run clean;npm run build",
        "clean": "tsc --build --clean",
        "version-beta": "npm version prerelease --preid beta",
        "publish-beta": "npm publish --tag beta",
        "docs": "typedoc --cleanOutputDir",
        "docs:watch": "typedoc --cleanOutputDir --watch",
        "specs:validate": "npx spec-box validate",
        "specs:sync": "npx spec-box sync"
    },
    "devDependencies": {
        "@spec-box/sync": "^1.2.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.10.1",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "prettier": "3.6.2",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.10",
        "typedoc-plugin-markdown": "^4.8.1",
        "typescript": "^5.9.2",
        "vite": "^7.2.6",
        "vite-plugin-dts": "^4.5.4"
    },
    "peerDependencies": {
        "@testing-library/dom": ">=10.4.1",
        "@testing-library/user-event": ">=14.6.1",
        "jsdom": ">=26.1.0"
    }
}
