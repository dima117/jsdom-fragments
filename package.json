{
    "name": "jsdom-fragments",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "format": "prettier --write ./**/*.ts",
        "build": "tsc --build tsconfig.publish.json",
        "prepare": "npm run clean;npm run build",
        "clean": "tsc --build --clean",
        "version-beta": "npm version prerelease --preid beta",
        "publish-beta": "npm publish --tag beta",
        "docs": "typedoc --cleanOutputDir",
        "docs:watch": "typedoc --cleanOutputDir --watch",
        "specs:validate": "npx spec-box validate",
        "specs:sync": "npx spec-box sync"
    },
    "author": "dima117a@gmail.com",
    "license": "MIT",
    "devDependencies": {
        "@spec-box/sync": "^1.2.0",
        "@types/jest": "^30.0.0",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "prettier": "3.6.2",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.10",
        "typedoc-plugin-markdown": "^4.8.1",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/user-event": "^14.6.1",
        "jsdom": "^26.1.0"
    }
}
